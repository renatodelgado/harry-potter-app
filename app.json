{
  "expo": {
    "name": "Harry Potter",
    "slug": "hp-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/harrypotterapp.jpg",
    "backgroundColor": "#000",
    "scheme": "hprn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/harrypotterapp.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/harrypotterapp.jpg",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/harrypotterapp.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
